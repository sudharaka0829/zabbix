<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>3.4</version>
    <date>2024-06-02T06:30:23Z</date>
    <groups>
        <group>
            <name>DBforBIX/Templates</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>DBforBix.DB2</template>
            <name>DBforBix.DB2</name>
            <description/>
            <groups>
                <group>
                    <name>DBforBIX/Templates</name>
                </group>
            </groups>
            <applications/>
            <items/>
            <discovery_rules>
                <discovery_rule>
                    <name>Bufferpools</name>
                    <type>0</type>
                    <key>db2.discovery.bufferpools</key>
                    <delay>30s</delay>
                    <status>0</status>
                    <params/>
                    <filter>
                        <evaltype>0</evaltype>
                        <conditions/>
                    </filter>
                    <lifetime>30d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Bufferpool {#BUFFERPOOL} index hit ratio</name>
                            <type>0</type>
                            <key>db2.bufferpool.idxhit[{#BUFFERPOOL}]</key>
                            <delay>30s</delay>
                            <history>90d</history>
                            <trends>365d</trends>
                            <status>0</status>
                            <value_type>0</value_type>
                            <units>percent</units>
                            <params/>
                        </item_prototype>
                        <item_prototype>
                            <name>Bufferpool {#BUFFERPOOL} total hit ratio</name>
                            <type>0</type>
                            <key>db2.bufferpool.totalhit[{#BUFFERPOOL}]</key>
                            <delay>30s</delay>
                            <history>90d</history>
                            <trends>365d</trends>
                            <status>0</status>
                            <value_type>0</value_type>
                            <units>percent</units>
                            <params/>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Databases</name>
                    <type>0</type>
                    <key>db2.discovery.databases</key>
                    <delay>30s</delay>
                    <status>0</status>
                    <params/>
                    <filter>
                        <evaltype>0</evaltype>
                        <conditions/>
                    </filter>
                    <lifetime>30d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>alive ({#DBNAME})</name>
                            <type>0</type>
                            <key>db2.alive[{#DBNAME}]</key>
                            <delay>30s</delay>
                            <history>90d</history>
                            <trends>365d</trends>
                            <status>0</status>
                            <value_type>3</value_type>
                            <units/>
                            <params/>
                        </item_prototype>
                    </item_prototypes>
                    <trigger_prototypes>
                        <trigger_prototype>
                            <expression>{DBforBix.DB2:db2.alive[{#DBNAME}].last()}<>1</expression>
                            <name>Database is down ({#DBNAME})</name>
                            <priority>3</priority>
                            <status>0</status>
                        </trigger_prototype>
                    </trigger_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Tablespaces</name>
                    <type>0</type>
                    <key>db2.discovery.tablespaces</key>
                    <delay>30s</delay>
                    <status>0</status>
                    <params/>
                    <filter>
                        <evaltype>1</evaltype>
                        <conditions>
                            <condition>
                                <macro>{#TABLESPACE}</macro>
                                <value>^(?!(TEMPSPACE1|SYSCATSPACE|USERSPACE1|SYSTOOLSPACE))</value>
                                <operator>8</operator>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <lifetime>30d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>tablespace total size KB ({#TABLESPACE})</name>
                            <type>0</type>
                            <key>db2.tablespace.totalkb[{#TABLESPACE}]</key>
                            <delay>30s</delay>
                            <history>90d</history>
                            <trends>365d</trends>
                            <status>0</status>
                            <value_type>0</value_type>
                            <units>KB</units>
                            <params/>
                        </item_prototype>
                        <item_prototype>
                            <name>tablespace used size KB ({#TABLESPACE})</name>
                            <type>0</type>
                            <key>db2.tablespace.usedkb[{#TABLESPACE}]</key>
                            <delay>30s</delay>
                            <history>90d</history>
                            <trends>365d</trends>
                            <status>0</status>
                            <value_type>0</value_type>
                            <units>KB</units>
                            <params/>
                        </item_prototype>
                    </item_prototypes>
                    <trigger_prototypes>
                        <trigger_prototype>
                            <expression>({DBforBix.DB2:db2.tablespace.totalkb[{#TABLESPACE}].last()}>0) and ({DBforBix.DB2:db2.tablespace.usedkb[{#TABLESPACE}].last()}/{DBforBix.DB2:db2.tablespace.totalkb[{#TABLESPACE}].last()}>0.8)</expression>
                            <name>Tablespace free is &lt; 20% ({#TABLESPACE})</name>
                            <priority>2</priority>
                            <status>0</status>
                        </trigger_prototype>
                    </trigger_prototypes>
                </discovery_rule>
            </discovery_rules>
            <httptests/>
            <macros/>
            <templates/>
            <screens/>
        </template>
    </templates>
</zabbix_export>
