    <groups>
        <group>
            <name>DBforBIX/Templates</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>DBforBix.DB2</template>
            <name>DBforBix.DB2</name>
            <description/>
            <groups>
                <group>
                    <name>DBforBIX/Templates</name>
                </group>
            </groups>
            <applications/>
            <items/>
            <discovery_rules>
                <discovery_rule>
                    <name>Bufferpools</name>
                    <type>11</type>
                    <snmp_community/>
                    <snmp_oid/>
                    <key>db2.discovery.DBforBIX.config[bufferpool,{$DSN}]</key>
                    <delay>30s</delay>
                    <status>1</status>
                    <allowed_hosts/>
                    <snmpv3_contextname/>
                    <snmpv3_securityname/>
                    <snmpv3_securitylevel>0</snmpv3_securitylevel>
                    <snmpv3_authprotocol>0</snmpv3_authprotocol>
                    <snmpv3_authpassphrase/>
                    <snmpv3_privprotocol>0</snmpv3_privprotocol>
                    <snmpv3_privpassphrase/>
                    <params><![CDATA[
                        <parms type="db2" prefix="db2.">
                            <server>
                                <discovery time="120" item="discovery.DBforBIX.config[bufferpool,{$DSN}]" names="BUFFERPOOL">select bp_name from SYSIBMADM.BP_HITRATIO where bp_name not like 'IBMSYSTEM%'</discovery>
                                <multiquery time="30" items="bufferpool.totalhit[%1]|bufferpool.idxhit[%1]" type="column" nodata="0">SELECT BP_NAME as bufferpool,TOTAL_HIT_RATIO_PERCENT AS totalhit, INDEX_HIT_RATIO_PERCENT AS idxhit FROM SYSIBMADM.BP_HITRATIO where bp_name not like 'IBMSYSTEM%'</multiquery>
                            </server>
                        </parms>
                    ]]></params>
                    <ipmi_sensor/>
                    <authtype>0</authtype>
                    <username/>
                    <password/>
                    <publickey/>
                    <privatekey/>
                    <port/>
                    <filter>
                        <evaltype>0</evaltype>
                        <formula/>
                        <conditions/>
                    </filter>
                    <lifetime>30d</lifetime>
                    <description/>
                    <item_prototypes>
                        <item_prototype>
                            <name>Bufferpool {#BUFFERPOOL} index hit ratio</name>
                            <type>2</type>
                            <snmp_community/>
                            <snmp_oid/>
                            <key>db2.bufferpool.idxhit[{#BUFFERPOOL}]</key>
                            <delay>0</delay>
                            <history>90d</history>
                            <trends>365d</trends>
                            <status>1</status>
                            <value_type>0</value_type>
                            <allowed_hosts/>
                            <units/>
                            <snmpv3_contextname/>
                            <snmpv3_securityname/>
                            <snmpv3_securitylevel>0</snmpv3_securitylevel>
                            <snmpv3_authprotocol>0</snmpv3_authprotocol>
                            <snmpv3_authpassphrase/>
                            <snmpv3_privprotocol>0</snmpv3_privprotocol>
                            <snmpv3_privpassphrase/>
                            <params/>
                            <ipmi_sensor/>
                            <authtype>0</authtype>
                            <username/>
                            <password/>
                            <publickey/>
                            <privatekey/>
                            <port/>
                            <description/>
                            <inventory_link>0</inventory_link>
                            <applications/>
                            <valuemap/>
                            <logtimefmt/>
                            <preprocessing/>
                            <jmx_endpoint/>
                            <application_prototypes/>
                            <master_item_prototype/>
                        </item_prototype>
                        <item_prototype>
                            <name>Bufferpool {#BUFFERPOOL} total hit ratio</name>
                            <type>2</type>
                            <snmp_community/>
                            <snmp_oid/>
                            <key>db2.bufferpool.totalhit[{#BUFFERPOOL}]</key>
                            <delay>0</delay>
                            <history>90d</history>
                            <trends>365d</trends>
                            <status>1</status>
                            <value_type>0</value_type>
                            <allowed_hosts/>
                            <units/>
                            <snmpv3_contextname/>
                            <snmpv3_securityname/>
                            <snmpv3_securitylevel>0</snmpv3_securitylevel>
                            <snmpv3_authprotocol>0</snmpv3_authprotocol>
                            <snmpv3_authpassphrase/>
                            <snmpv3_privprotocol>0</snmpv3_privprotocol>
                            <snmpv3_privpassphrase/>
                            <params/>
                            <ipmi_sensor/>
                            <authtype>0</authtype>
                            <username/>
                            <password/>
                            <publickey/>
                            <privatekey/>
                            <port/>
                            <description/>
                            <inventory_link>0</inventory_link>
                            <applications/>
                            <valuemap/>
                            <logtimefmt/>
                            <preprocessing/>
                            <jmx_endpoint/>
                            <application_prototypes/>
                            <master_item_prototype/>
                        </item_prototype>
                    </item_prototypes>
                    <trigger_prototypes/>
                    <graph_prototypes/>
                    <host_prototypes/>
                    <jmx_endpoint/>
                </discovery_rule>
                <discovery_rule>
                    <name>Databases</name>
                    <type>11</type>
                    <snmp_community/>
                    <snmp_oid/>
                    <key>db2.discovery.DBforBIX.config[database,{$DSN}]</key>
                    <delay>30s</delay>
                    <status>0</status>
                    <allowed_hosts/>
                    <snmpv3_contextname/>
                    <snmpv3_securityname/>
                    <snmpv3_securitylevel>0</snmpv3_securitylevel>
                    <snmpv3_authprotocol>0</snmpv3_authprotocol>
                    <snmpv3_authpassphrase/>
                    <snmpv3_privprotocol>0</snmpv3_privprotocol>
                    <snmpv3_privpassphrase/>
                    <params><![CDATA[
                        <parms type="db2" prefix="db2.">
                            <server>
                                <discovery time="120" item="discovery.DBforBIX.config[database,{$DSN}]" names="DBNAME">select current server as server from sysibm.sysdummy1</discovery>
                                <query time="30" item="alive[%1]" nodata="0">select current server as server, 1 from sysibm.sysdummy1</query>
                            </server>
                        </parms>
                    ]]></params>
                    <ipmi_sensor/>
                    <authtype>0</authtype>
                    <username/>
                    <password/>
                    <publickey/>
                    <privatekey/>
                    <port/>
                    <filter>
                        <evaltype>0</evaltype>
                        <formula/>
                        <conditions/>
                    </filter>
                    <lifetime>30d</lifetime>
                    <description/>
                    <item_prototypes>
                        <item_prototype>
                            <name>alive ({#DBNAME})</name>
                            <type>2</type>
                            <snmp_community/>
                            <snmp_oid/>
                            <key>db2.alive[{#DBNAME}]</key>
                            <delay>0</delay>
                            <history>90d</history>
                            <trends>365d</trends>
                            <status>0</status>
                            <value_type>3</value_type>
                            <allowed_hosts/>
                            <units/>
                            <snmpv3_contextname/>
                            <snmpv3_securityname/>
                            <snmpv3_securitylevel>0</snmpv3_securitylevel>
                            <snmpv3_authprotocol>0</snmpv3_authprotocol>
                            <snmpv3_authpassphrase/>
                            <snmpv3_privprotocol>0</snmpv3_privprotocol>
                            <snmpv3_privpassphrase/>
                            <params/>
                            <ipmi_sensor/>
                            <authtype>0</authtype>
                            <username/>
                            <password/>
                            <publickey/>
                            <privatekey/>
                            <port/>
                            <description/>
                            <inventory_link>0</inventory_link>
                            <applications/>
                            <valuemap/>
                            <logtimefmt/>
                            <preprocessing/>
                            <jmx_endpoint/>
                            <application_prototypes/>
                            <master_item_prototype/>
                        </item_prototype>
                    </item_prototypes>
                    <trigger_prototypes>
                        <trigger_prototype>
                            <expression>{DBforBix.DB2:db2.alive[{#DBNAME}].last()}&lt;&gt;1</expression>
                            <recovery_mode>0</recovery_mode>
                            <recovery_expression/>
                            <name>Database is down ({#DBNAME})</name>
                            <correlation_mode>0</correlation_mode>
                            <correlation_tag/>
                            <url/>
                            <status>0</status>
                            <priority>3</priority>
                            <description/>
                            <type>0</type>
                            <manual_close>0</manual_close>
                            <dependencies/>
                            <tags/>
                        </trigger_prototype>
                    </trigger_prototypes>
                    <graph_prototypes/>
                    <host_prototypes/>
                    <jmx_endpoint/>
                </discovery_rule>
                <!-- Add more discovery rules if needed -->
            </discovery_rules>
            <httptests/>
            <macros/>
            <templates/>
            <screens/>
        </template>
    </templates>
